{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/chat-gateway/src",
  "monorepo": true,
  "root": "apps/chat-gateway",
  "compilerOptions": {
    "webpack": false,
    "tsConfigPath": "apps/chat-gateway/tsconfig.app.json"
  },
  "projects": {
    "chat-gateway": {
      "type": "application",
      "root": "apps/chat-gateway",
      "entryFile": "main",
      "sourceRoot": "apps/chat-gateway/src",
      "compilerOptions": {
        "tsConfigPath": "apps/chat-gateway/tsconfig.app.json"
      }
    },
    "chat-worker": {
      "type": "application",
      "root": "apps/chat-worker",
      "entryFile": "main",
      "sourceRoot": "apps/chat-worker/src",
      "compilerOptions": {
        "tsConfigPath": "apps/chat-worker/tsconfig.app.json"
      }
    },
    "common": {
      "type": "library",
      "root": "libs/common",
      "entryFile": "index",
      "sourceRoot": "libs/common/src",
      "compilerOptions": {
        "tsConfigPath": "libs/common/tsconfig.lib.json"
      }
    },
    "db": {
      "type": "library",
      "root": "libs/db",
      "entryFile": "index",
      "sourceRoot": "libs/db/src",
      "compilerOptions": {
        "tsConfigPath": "libs/db/tsconfig.lib.json"
      }
    },
    "redis": {
      "type": "library",
      "root": "libs/redis",
      "entryFile": "index",
      "sourceRoot": "libs/redis/src",
      "compilerOptions": {
        "tsConfigPath": "libs/redis/tsconfig.lib.json"
      }
    }
  }
}
